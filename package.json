{
  "name": "item-selection",
  "version": "1.0.1",
  "description": "Manage item multi-selections.",
  "license": "MIT",
  "repository": "goto-bus-stop/item-selection",
  "author": "Ren√© Kooi <rene@kooi.me>",
  "main": "./immutable",
  "jsnext:main": "es/immutable",
  "module": "es/immutable",
  "scripts": {
    "build:es": "babel src --out-dir es",
    "build:lib": "babel --plugins transform-es2015-modules-commonjs es --out-dir .",
    "build": "npm run build:es && npm run build:lib",
    "example": "BABEL_ENV=example browserify --debug -t [ babelify ] -r ./:item-selection -e example.js -o example.build.js",
    "prepublish": "npm run build",
    "test": "standard && ava ./test.js"
  },
  "files": [
    "immutable.js",
    "mutable.js"
  ],
  "keywords": [
    "selection",
    "range",
    "list",
    "collection"
  ],
  "dependencies": {
    "ranged": "^0.0.3"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "deku": "^1.0.0",
    "virtual-element": "^1.2.0"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ],
    "env": {
      "example": {
        "plugins": [
          "syntax-jsx",
          [
            "transform-react-jsx",
            {
              "pragma": "element"
            }
          ],
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  }
}
