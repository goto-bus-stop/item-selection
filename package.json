{
  "name": "item-selection",
  "version": "1.2.0",
  "description": "Manage item multi-selections.",
  "license": "MIT",
  "repository": "goto-bus-stop/item-selection",
  "author": "Ren√© Kooi <rene@kooi.me>",
  "main": "./immutable",
  "jsnext:main": "es/immutable",
  "module": "es/immutable",
  "scripts": {
    "build:es": "babel src --out-dir es",
    "build:lib": "babel --plugins @babel/plugin-transform-modules-commonjs es --out-dir .",
    "build": "npm-run-all build:es build:lib",
    "example": "cross-env BABEL_ENV=example browserify --debug -t babelify -r ./:item-selection -e example.js -o example.build.js",
    "prepublish": "npm-run-all build",
    "test:lint": "standard",
    "test:tape": "tape -r @babel/register ./test.js",
    "test": "npm-run-all test:*"
  },
  "files": [
    "README.md",
    "LICENSE",
    "/immutable.js",
    "/mutable.js",
    "/es/immutable.js",
    "/es/mutable.js"
  ],
  "keywords": [
    "selection",
    "range",
    "list",
    "collection"
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.0",
    "cross-env": "^5.1.4",
    "deku": "^1.0.0",
    "npm-run-all": "^4.0.2",
    "standard": "^12.0.0",
    "tape": "^4.6.3",
    "virtual-element": "^1.2.0"
  }
}
