{ "name": "item-selection"
, "version": "1.0.1"
, "description": "Manage item multi-selections."
, "license": "MIT"
, "repository": "goto-bus-stop/item-selection"
, "author": "Ren√© Kooi <rene@kooi.me>"
, "main": "immutable"
, "scripts":
  { "build": "babel --presets es2015 --out-dir . src"
  , "example": "browserify --debug -t [ babelify ] -r ./:item-selection -e example.js -o example.build.js"
  , "prepublish": "npm run build"
  , "test": "standard && ava ./test.js" }
, "files": [ "immutable.js", "mutable.js" ]
, "keywords":
  [ "selection"
  , "range"
  , "list"
  , "collection" ]
, "dependencies":
  { "ranged": "^0.0.3" }
, "devDependencies":
  { "ava": "^0.17.0"
  , "babel-cli": "6.x"
  , "babel-preset-es2015": "6.x"
  , "babel-plugin-syntax-jsx": "6.x"
  , "babel-plugin-transform-react-jsx": "6.x"
  , "babelify": "7.x"
  , "browserify": "13.1.1"
  , "deku": "^1.0.0"
  , "virtual-element": "^1.2.0" }
, "babel":
  { "presets":
    [ "es2015" ]
  , "plugins":
    [ "syntax-jsx"
    , [ "transform-react-jsx", { "pragma": "element" } ] ] } }
